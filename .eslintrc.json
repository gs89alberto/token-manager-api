{
  "parser": "@typescript-eslint/parser",
  "env": {
    "node": true
  },  
  "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "google",
      "prettier"
    ],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error"
    },
    "overrides": [
      {
        "files": ["*.ts"]
      }
    ]
  }